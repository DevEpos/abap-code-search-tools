 <asx:abap xmlns:asx="http://www.sap.com/abapxml" version="1.0">
  <asx:values>
   <VSEOCLASS>
    <CLSNAME>ZCL_ADCOSET_SEARCH_PROTOCOL</CLSNAME>
    <LANGU>E</LANGU>
    <DESCRIPT>Protocol that collects information during code search</DESCRIPT>
    <STATE>1</STATE>
    <CLSCCINCL>X</CLSCCINCL>
    <FIXPT>X</FIXPT>
    <UNICODE>X</UNICODE>
   </VSEOCLASS>
   <DESCRIPTIONS>
    <SEOCOMPOTX>
     <CMPNAME>ADD_LOC</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Adds Lines of Code to total searched LoC</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>GET_LOC</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Retrieves searched Lines of Code</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>GET_SEARCHED_OBJECT_COUNT</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Returns count of searched objects</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>GET_SEARCHED_SOURCES_COUNT</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Returns count of searched sources</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>INCREASE_SEARCHD_OBJECTS_COUNT</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Increases count of searched objects with the given value</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>INCREASE_SEARCHD_SOURCES_COUNT</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Increases count of searched sources with the given value</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>INCREMENT_SEARCHED_OBJS_COUNT</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Increments the count of searched objects</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>INCREMENT_SEARCHED_SRCS_COUNT</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Increments the count of searched sources</DESCRIPT>
    </SEOCOMPOTX>
    <SEOCOMPOTX>
     <CMPNAME>RESET</CMPNAME>
     <LANGU>E</LANGU>
     <DESCRIPT>Resets the protocol values</DESCRIPT>
    </SEOCOMPOTX>
   </DESCRIPTIONS>
  </asx:values>
 </asx:abap>
